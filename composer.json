{
  "name": "tearoom1/uniform-spam-words",
  "description": "Simple spam guard for 'mzur/kirby-uniform' & Kirby",
  "license": "MIT",
  "type": "kirby-plugin",
  "homepage": "https://github.com/tearoom1/uniform-spam-words",
  "version": "1.4.1",
  "keywords": [
    "kirby",
    "form"
  ],
  "authors": [
    {
      "name": "Mathis Koblin",
      "email": "mail@tearoom.one",
      "homepage": "https://www.tearoom.one",
      "role": "Maintainer"
    }
  ],
  "support": {
    "docs": "https://github.com/tearoom1/uniform-spam-words/blob/main/README.md",
    "source": "https://github.com/tearoom1/uniform-spam-words"
  },
  "require": {
    "getkirby/composer-installer": "^1.1",
    "mzur/kirby-uniform": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.4.3",
    "getkirby/cms": "^4.1.0"
  },
  "autoload": {
    "psr-4": {
      "Uniform\\Guards\\SpamWordsGuard\\": "src/guards/SpamWordsGuard.php"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "getkirby/composer-installer": true
    }
  },
  "scripts": {
    "dist": "composer install --no-dev --optimize-autoloader",
    "test": "vendor/bin/phpunit",
    "test-local": "vendor/bin/phpunit --testdox"
  },
  "funding": [
    {
      "type": "Buy me a coffee",
      "url": "https://buymeacoffee.com/tearoom1"
    },
    {
      "type": "Github Sponsors",
      "url": "https://github.com/sponsors/tearoom1"
    }
  ]
}

{
  "name": "tearoom1/uniform-spam-words",
  "description": "Simple spam guard for 'mzur/kirby-uniform' & Kirby",
  "type": "kirby-plugin",
  "license": "MIT",
  "author": {
    "name": "Mathis Koblin",
    "email": "mail@tearoom.one",
    "homepage": "https://www.tearoom.one",
    "role": "Maintainer"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tearoom1/uniform-spam-words.git"
  },
  "require": {
    "php": "~8.1.0 || ~8.2.0 || ~8.3.0",
    "getkirby/composer-installer": "^1.1",
    "mzur/kirby-uniform": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Uniform\\Guards\\SpamGuard\\": "src/guards/SpamGuard.php"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "getkirby/composer-installer": true
    }
  }
}
